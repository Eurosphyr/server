{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/node"
      },
      {
        "src": "dist/**",
        "use": "@vercel/static"
      }
    ],
    "rewrites": [
      { "source": "/(.*)", "destination": "dist/$1.js" },
      { "source": "/.*", "destination": "/dist/index.js" }
    ]
  }
  