{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/node"
      },
      {
        "src": "dist/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      { "handle": "filesystem" },
      { "src": "/(.*)", "dest": "dist/$1.js" },
      { "src": "/.*", "dest": "/dist/index.js" }
    ],
    "rewrites": [
          { "source": "/(.*)", "destination": "/dist/$1.js" },
          { "src": "/.*", "dest": "/dist/index.js" }
        ]
  }
  